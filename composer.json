{
    "name": "nicmart/tree",
    "description": "A basic but flexible php tree data structure and a fluent tree builder implementation.",
    "license": "MIT",
    "type": "library",
    "keywords": [],
    "authors": [
        {
            "name": "Nicol√≤ Martini",
            "email": "nicmartnic@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.19.0",
        "ergebnis/license": "^1.1.0",
        "ergebnis/php-cs-fixer-config": "^2.14.0",
        "phpunit/phpunit": "^8.5.31",
        "psalm/plugin-phpunit": "~0.18.1",
        "vimeo/psalm": "^4.30.0"
    },
    "autoload": {
        "psr-4": {
            "Tree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tree\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "7.2.34"
        },
        "sort-packages": true
    }
}
