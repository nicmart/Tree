{
    "name": "nicmart/tree",
    "description": "A basic but flexible php tree data structure and a fluent tree builder implementation.",
    "license": "MIT",
    "type": "library",
    "keywords": [],
    "authors": [
        {
            "name": "Nicolò Martini",
            "email": "nicmartnic@gmail.com"
        },
        {
            "name": "Andreas Möller",
            "email": "am@localheinz.com"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.29.0",
        "ergebnis/license": "^2.1.0",
        "ergebnis/php-cs-fixer-config": "^5.2.0",
        "phpunit/phpunit": "^9.5.28",
        "psalm/plugin-phpunit": "~0.18.4",
        "vimeo/psalm": "^5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Tree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tree\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.0.25"
        },
        "sort-packages": true
    }
}
